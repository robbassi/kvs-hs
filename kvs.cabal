cabal-version: 2.2
name: kvs
version: 0.0.1
license: MIT
license-file: LICENSE
build-type: Simple

executable kvs
    main-is: Main.hs
    default-language: Haskell2010
    hs-source-dirs:
      src
    other-modules:
      Kvs
      CommitLog
      Memtable
      Segments
      Types
    default-extensions:
      RecordWildCards
      OverloadedStrings
    build-depends:
      , base < 5
      , mtl
      , text
      , bytestring
      , concurrent-extra
